<%!IF:<%SESSION:login_id:SESSION%>:<!%EVAL:header("location : //yahoo.com"):EVAL%>::IF%>
<main class="bs-docs-masthead" role="main">
	<form method="post" action="<%CLASS:URL:getDir:CLASS%>login.php">
	<input type="hidden" name="login" value="login">

	<div class="login">
		<div class="input-area">
			<div class="input-group">
				<span class="input-group-addon">ID</span>
				<input class="form-control" type="text" name="login_id" autocomplete="off">
			</div>
			<div class="input-group">
				<span class="input-group-addon">Password</span>
				<input class="form-control" type="password" name="login_pw" autocomplete="off">
			</div>
			<div class="login-submit">
				<button class="btn btn-default"><span>Login</span></button>
			</div>
			<div class="user_add">
				<a href="<%CLASS:URL:getDir:CLASS%>?p=account_add">新規登録</a>
			</div>
		</div>
	</div>
</form>
</main>
